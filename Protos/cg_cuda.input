
L=SED_NL
T=SED_NT

DebugLevel = 1
InitialStoreCounter = SED_Conf
Measurements = 1
2kappamu = SED_2kappamu
Kappa = SED_Kappa
BCAngleT = 1.0
ThetaX = SED_ThetaX
ThetaY = SED_ThetaY
ThetaZ = SED_ThetaZ
GaugeConfigInputFile = SED_GaugeConfigInputFile
UseEvenOdd = yes

NrXProcs = SED_NrXProcs
NrYProcs = SED_NrYProcs
NrZProcs = SED_NrZProcs


ReadSource = yes
NoSamples = 1
Indices = 0-SED_IndexEnd
UseRelativePrecision = yes
SplittedPropagator = yes

BeginMonomial GAUGE
  beta = SED_Beta
  Type = Wilson
  Timescale = 0
EndMonomial

BeginGPUInit
  MaxInnerSolverIteration = 1500
  InnerSolverPrecision = 1.e-6
EndGPUInit

BeginOperator TMWILSON
  2kappaMu = SED_2kappamu
  kappa = SED_Kappa
  UseEvenOdd = no
  Solver = cg
  SolverPrecision = SED_SolverPrecision
  MaxSolverIterations = 100000
  PropagatorPrecision = 32
EndOperator
